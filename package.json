  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "ava-nock": "latest",
    "babel-eslint": "latest",
    "conventional-github-releaser": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "glob": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "NODE_ENV=production next build",
    "clean": "rm -rf node_modules && rm -rf .next",
    "deploy": "npm run docker:build && npm run docker:push && npm run helm:deploy",
    "dev": "node server.js",
    "lint": "standard-markdown && standard",
    "postrelease": "npm run release:tags && npm run release:github",
    "precommit": "lint-staged",
    "prerelease": "npm run update:check && git-authors-cli",
    "pretest": "npm run lint",
    "release": "git add package.json && standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "start": "NODE_ENV=production node server.js",
    "test": "GITHUB_TOKEN=FAKE_GITHUB_TOKEN NODE_ENV=test ava",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "private": true,
  "license": "Apache-2.0",
  "ava": {
    "require": [
      "esm"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ],
    "*.md": [
      "standard-markdown",
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "standard-version": {
    "scripts": {
      "prechangelog": "git-authors-cli"
    }
  }
}
